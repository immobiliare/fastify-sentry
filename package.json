{
    "name": "@immobiliare/fastify-sentry",
    "version": "1.0.0-0",
    "description": "Simple fastify plugin to integrates Sentry error reporting into your services",
    "main": "index.js",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "nodejs",
        "javascript",
        "npm",
        "sentry",
        "errors",
        "fastify",
        "error-handling",
        "plugin"
    ],
    "files": [
        "index.js",
        "allowedStatusCodes.js",
        "index.d.ts"
    ],
    "scripts": {
        "lint": "eslint --fix --ignore-path .gitignore .",
        "test": "tsd && nyc --reporter=lcov --reporter=text-summary ava && nyc check-coverage --lines 95 --branches 80 --functions 95 --statements 95",
        "bench": "npm run bench:base && npm run bench:plugin",
        "bench:base": "npx concurrently -k -s first \"node ./benchmarks/base.js\" \"node -e 'setTimeout(() => {}, 1000)' && npx autocannon localhost:4001\"",
        "bench:plugin": "npx concurrently -k -s first \"node ./benchmarks/sentry-mock.js\" \"node ./benchmarks/with-sentry.js\" \"node -e 'setTimeout(() => {}, 1000)' && npx autocannon localhost:4002\"",
        "toc": "markdown-toc -i README.md",
        "prepare": "node prepare.js || echo 'Skipping prepare script'"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/immobiliare/fastify-sentry.git"
    },
    "homepage": "https://github.com/immobiliare/fastify-sentry",
    "bugs": {
        "url": "https://github.com/immobiliare/fastify-sentry/issues"
    },
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.0.3",
        "@commitlint/config-conventional": "^16.0.0",
        "@types/node": "^17.0.2",
        "autocannon": "^7.4.0",
        "ava": "^3.9.0",
        "concurrently": "^7.0.0",
        "cz-conventional-changelog": "^3.2.0",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fastify": "^3.22.0",
        "husky": "^7.0.0",
        "is-ci": "^3.0.0",
        "lint-staged": "^12.0.0",
        "markdown-toc": "^1.2.0",
        "nyc": "^15.0.1",
        "prettier": "^2.4.1",
        "sinon": "^12.0.0",
        "tsd": "^0.19.0",
        "typescript": "^4.4.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@sentry/node": "^6.13.3",
        "fastify-plugin": "^3.0.0"
    }
}
